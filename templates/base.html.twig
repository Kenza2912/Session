<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text><text y=%221.3em%22 x=%220.2em%22 font-size=%2276%22 fill=%22%23fff%22>sf</text></svg>">
        {% block stylesheets %}
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootswatch@5.1.3/dist/united/bootstrap.min.css">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootswatch@5.1.3/dist/united/_variables.scss">
        <link rel="stylesheet" href="{{ asset('styles/app.css') }}">
            
        {% endblock %}

        {% block javascripts %}
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js" integrity="sha512-v2CJ7UaYy4JwqLDIrZUI/4hqeoQieOmAZNXBeQyjo21dadnwR+8ZaIJVT8EE2iyI61OV8e6M8PP2/4hpQINQ/g==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
        <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.min.js" integrity="sha384-Atwg2Pkwv9vp0ygtn1JAojH0nYbwNJLPhwyoVbhoPwBhjQPR5VtM2+xf0Uwh9KtT" crossorigin="anonymous"></script>
        <script src="{{ asset('js/gsf.js')}}"></script>
          
            {% block importmap %}{{ importmap('app') }}{% endblock %}
        {% endblock %}
    </head>
    <body>
        {% for message in app.flashes('success') %}
			<div class="alert alert-success mt-4">
				{{ message }}
			</div>
           
		{% endfor %}

        <nav class="">
            <div class="">
              
              
              <div class="">
                <ul class="">
                  <li class="">
                    <a class="" aria-current="page" href="{{path('app_category')}}">Catégorie</a>
                  </li>
                  <li class="">
                    <a class="" href="{{path('app_modul')}}">Module</a>
                  </li>
                  <li class="">
                    <a class="" href="{{path('app_trainee')}}">Stagiaires</a>
                  </li>
                  <li class="">
                    <a class="" href="{{path('app_session')}}">Session</a>
                  </li>
                  <li class="">
                    <a class="" href="{{path('app_training')}}">Formation</a>
                  </li>
                </ul>
                <ul class="navbar-nav">
                  {% if app.user %}
                      <li class="">
                          <a class="" href="">Profil</a>
                      </li>
                      <li class="">
                          <a class="" href="{{ path('app_logout') }}">Déconnexion</a>
                      </li>
                  {% else %}
                      <li class="">
                          <a class="" href="{{ path('app_login') }}">Connexion</a>
                      </li>
                      <li class="">
                          <a class="" href="{{ path('app_register') }}">Inscription</a>
                      </li>
                  {% endif %}
              </ul>
              </div>
            </div>
          </nav>

          
        {% block body %}{% endblock %}
    </body>
</html>
